{
  "name": "db",
  "version": "1.0.0",
  "description": "Database client and schema",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "generate": "npx dotenv -e ../../.env -- prisma generate",
    "push": "npx dotenv -e ../../.env -- prisma db push",
    "migrate:dev": "npx dotenv -e ../../.env -- prisma migrate dev",
    "migrate:deploy": "npx dotenv -e ../../.env -- prisma migrate deploy",
    "migrate:reset": "npx dotenv -e ../../.env -- prisma migrate reset",
    "seed": "npx dotenv -e ../../.env -- tsx prisma/seed.ts",
    "studio": "npx dotenv -e ../../.env -- prisma studio",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0"
  },
  "devDependencies": {
    "prisma": "^5.6.0",
    "typescript": "^5.2.2",
    "@types/node": "^20.8.6",
    "tsx": "^4.0.0",
    "eslint": "^8.51.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0"
  }
}
